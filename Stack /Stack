#include<stdio.h>
#define MAX 5
int stack[MAX];
int top=-1;
int main()
{
    int n;
    printf("enter the elements(max %d):",MAX);
    scanf("%d",&n);
    if(n>MAX)
    {
    printf("stack is overflow");
    return 0;
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d",&stack[i]);
        top++;
    }
    if(top==-1)
    {
        printf("underflow");
    }
    else
    {
        printf("popped elements %d\n",stack[top]);
        top--;
    }
    printf("stack after pop");
    if(top==-1)
    {
        printf("stack is empty");
    }
    else
    {
        for(int i=top;i>=0;i--)
        {
            printf("%d",stack[i]);
        }
    }
    
    
    
}
